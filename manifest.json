{
  "name": "MyQuiz",
  "author": "Anh Phê",
   "version": "0.1.4",
  "manifest_version": 3,
  "description": "Ôn thi trắc nghiệm.\nPhím tắt Alt+Q",
   "icons": {
      "128": "src/img/128.png",
      "16": "src/img/16.png",
      "32": "src/img/32.png",
      "48": "src/img/48.png"
   },
  "permissions": [
    "storage"
  ],
 "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+Q",
        "mac": "Command+Q"
      },
     "description": "Ôn thi trắc nghiệm"
    }
},
    "background": {
        "service_worker": "background.js"
    },
   "action": {}
}
